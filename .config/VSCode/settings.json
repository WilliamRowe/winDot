{
    "security.allowedUNCHosts": [
        "wsl.localhost",
        "wsl$",
        "p47rtblds01"
    ],
    "liveServer.settings.donotShowInfoMsg": true,
    "remote.autoForwardPortsSource": "hybrid",
    "telemetry.telemetryLevel": "off",
    "workbench.colorCustomizations": {},
    "workbench.iconTheme": "Aura Dark",
    "workbench.colorTheme": "Aura Dark",
    "workbench.startupEditor": "none",
    "workbench.sideBar.location": "right",
    // "workbench.layoutControl.enabled": false,
    // "workbench.statusBar.visible": false,
    "workbench.activityBar.location": "hidden",
    "workbench.editor.empty.hint": "hidden",
    //"window.menuBarVisibility": "",
    "window.commandCenter": false,
    "window.zoomLevel": 0.0,
    "terminal.integrated.minimumContrastRatio": 1,
    "terminal.integrated.cwd": "${workspaceFolder}",
    "terminal.integrated.fontSize": 14,
    "editor.lineHeight": 1.5,
    "editor.mouseWheelZoom": true,
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.linkedEditing": true,
    "editor.formatOnSave": true,
    "editor.smoothScrolling": true,
    "editor.fontLigatures": false,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "explorer.compactFolders": false,
    "explorer.decorations.colors": false,
    "files.autoSave": "afterDelay",
    "git.defaultCloneDirectory": "${userHome}\\repo\\",
    "git.autofetch": true,
    "git.openRepositoryInParentFolders": "never",
    "[html]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "powershell.cwd": "${workspaceFolder}",
    "powershell.powerShellDefaultVersion": "",
    "powershell.promptToUpdatePowerShell": true,
    "powershell.enableProfileLoading": false,
    "powershell.startAutomatically": true,
    "powershell.codeFormatting.avoidSemicolonsAsLineTerminators": true,
    "powershell.codeFormatting.autoCorrectAliases": true,
    "powershell.codeFormatting.useConstantStrings": true,
    "powershell.codeFormatting.useCorrectCasing": true,
    "powershell.integratedConsole.startInBackground": true,
    "powershell.codeFormatting.pipelineIndentationStyle": "IncreaseIndentationForFirstPipeline",
    "powershell.codeFormatting.openBraceOnSameLine": true,
    "powershell.codeFormatting.newLineAfterOpenBrace": true,
    "powershell.codeFormatting.newLineAfterCloseBrace": false,
    "powershell.codeFormatting.preset": "OTBS",
    "powershell.codeFormatting.whitespaceBetweenParameters": true,
    "powershell.scriptAnalysis.settingsPath": "PSScriptAnalyzerSettings.psd1",
    "powershell.pester.outputVerbosity": "Detailed",
    "powershell.pester.debugOutputVerbosity": "Detailed",
    "code-runner.clearPreviousOutput": true,
    "code-runner.runInTerminal": true,
    "highlightDuplicates.active": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.integrated.allowedLinkSchemes": [
        "file",
        "http",
        "https",
        "mailto",
        "vscode",
        "vscode-insiders",
        "ms-settings"
    ],
    "powershell.pester.codeLens": false,
    "cSpell.userWords": [
        "mstsc",
        "Runas"
    ],
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "projectManager.sortList": "Saved"
    // "chat.viewSessions.orientation": "stacked"
}